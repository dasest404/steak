{
    "name": "parsnick/steak",
    "description": "Static site generator using blade syntax and gh-pages",
    "license": "MIT",
    "version": "1.0.0-dev",
    "authors": [
        {
            "name": "Nick",
            "email": "nick.dev@mailingaddress.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Parsnick\\Steak\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Parsnick\\Steak\\Tests\\": "tests"
        }
    },
    "bin": [
        "steak"
    ],
    "require": {
        "cpliakas/git-wrapper": "^1.6",
        "illuminate/config": "^5.2",
        "illuminate/container": "^5.2",
        "illuminate/filesystem": "^5.2",
        "illuminate/pipeline": "^5.2",
        "illuminate/view": "^5.2",
        "michelf/php-markdown": "^1.6",
        "mikey179/vfsStream": "^1.6",
        "nesbot/carbon": "^1.21",
        "symfony/console": "^3.0",
        "symfony/debug": "^3.0",
        "symfony/stopwatch": "^3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.4",
        "phpunit/phpunit": "^5.2",
        "raveren/kint": "^1.0"
    }
}
